<mvc:View
	controllerName="co.h2a.sigpro.ui.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="sap-icon://document"
		id="page"
		description="">
		<buttons>
			<Button id="toolpageBtnLink" text="{i18n>backToToolpage}" press=".onLinkToolpagePressed"  type="Transparent"/>
		</buttons>
	</MessagePage>

</mvc:View>